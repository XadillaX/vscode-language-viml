{
  "comments": {
    "lineComment": "# ",
  },
  "brackets": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ]
  ],
  "autoClosingPairs": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ],
    [ "'", "'" ],
    [ "\"", "\"" ]
  ],
  "surroundingPairs": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ],
    [ "'", "'" ],
    [ "\"", "\"" ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^snippet.+(?<!\")",
    "decreaseIndentPattern": "^[^ ]$",
    "comment": "^[^ ]$ is a noop regexp"
  }
}
